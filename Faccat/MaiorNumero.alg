algoritmo "MaiorNumero"
var
   num1, num2: real
inicio
      escreva("Digite o primeiro valor: ")
      leia(num1)
      escreva("Digite o segundo valor: ")
      leia(num2)
      se num1 > num2 entao
        escreva("O maior valor é ", num1)
      senao
        escreva("O maior valor é ", num2)
      fimse
fimalgoritmo
