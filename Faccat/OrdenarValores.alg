algoritmo "OrdenarValores"
var

inicio
      escreva("Digite o primeiro valor: ")
      leia(num1)
      escreva("Digite o segundo valor: ")
      leia(num2)

      se num1 > num2 entao
        escreva("Valores em ordem crescente: ", num2, ", ", num1)
      senao
        escreva("Valores em ordem crescente: ", num1, ", ", num2)
      fimse
fimalgoritmo
