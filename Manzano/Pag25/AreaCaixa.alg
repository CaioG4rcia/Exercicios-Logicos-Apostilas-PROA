algoritmo "AreaCaixa"
var
   comp, l, a, volume: real
inicio
      escreva("Digite o comprimento da caixa: ")
      leia(comp)
      escreva("Digite a largura da caixa: ")
      leia(l)
      escreva("Digite a altura da caixa: ")
      leia(a)
      volume <- comp * l * a
      escreva("Area da caixa: ", volume)
fimalgoritmo
