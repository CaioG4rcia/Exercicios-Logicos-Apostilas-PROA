algoritmo "SomarQuadrados"
var
   a, b, c, soma: real
inicio
      escreva("Digite o valor de a: ")
      leia(a)
      escreva("Digite o valor de b: ")
      leia(b)
      escreva("Digite o valor de c: ")
      leia(c)
      soma <- (a ^ 2) + (b ^ 2) + (c ^ 2)
      escreva("A soma dos quadrados foi de: ", soma)
fimalgoritmo
